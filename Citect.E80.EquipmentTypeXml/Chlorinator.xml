    <field name="sampleper" />
    <field name="files" />
    <field name="time" />
    <field name="period" />
    <field name="comment">SFC Chlorine (mg/l) trend</field>
    <field name="type">TRN_PERIODIC</field>
    <field name="stormethod">Floating Point (8-byte samples)</field>
